{
  "name": "chuss",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^2.3.5",
    "@cyntler/react-doc-viewer": "^1.8.0",
    "@electron/remote": "^2.0.8",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@reduxjs/toolkit": "^1.8.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "axios": "^1.1.3",
    "chart.js": "^3.9.1",
    "electron-is-dev": "^2.0.0",
    "form-data": "^4.0.0",
    "formik": "^2.2.9",
    "framer-motion": "^7.5.3",
    "js-cookie": "^3.0.1",
    "moment-timezone": "^0.5.39",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-color": "^2.19.3",
    "react-csv": "^2.2.2",
    "react-doc-viewer": "^0.1.5",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-phone-input-2": "^2.15.1",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "socket.io-client": "^4.5.3",
    "styled-components": "^5.3.6",
    "web-vitals": "^2.1.0",
    "xlsx": "^0.18.5",
    "yup": "^0.32.11"
  },
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none yarn start\" \"yarn electron:start\"",
    "electron:build": "yarn build && electron-builder -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on tcp:3000 && electron ."
  },
  "build": {
    "generateUpdatesFilesForAllChannels": true,
    "extends": null,
    "appId": "com.jkimbareeba.chuss",
    "productName": "CHUSS",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "./build/chuss512.ico"
    },
    "nsis": {
      "oneClick": false,
      "installerIcon": "./build/chuss512.ico",
      "uninstallerIcon": "./build/chuss512.ico",
      "uninstallDisplayName": "chuss-uninstall",
      "allowToChangeInstallationDirectory": true
    },
    "mac": {
      "category": "public.app-category.education",
      "target": "dmg",
      "icon": "./build/chuss256.icns"
    },
    "files": [
      "dist/**/**",
      "build/**/**",
      "node_modules/**/**",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "electron": "^21.1.1",
    "electron-builder": "^23.6.0",
    "wait-on": "^6.0.1"
  },
  "author": "Joshua Kimbareeba"
}
